{
  "name": "subpirate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "stripe:webhook": "ts-node scripts/setup-stripe-webhook.ts",
    "stripe:sync": "node src/lib/stripe/sync-products.js",
    "stripe:migrate": "node scripts/apply-basejump-migrations.js",
    "stripe:setup": "npm run stripe:migrate && npm run stripe:sync",
    "server": "node server.js",
    "webhook": "node webhook-server.js",
    "campaigns:scheduler": "node webhook-server.js",
    "campaigns:run": "cross-env NODE_ENV=production node webhook-server.js",
    "campaigns:dev": "cross-env NODE_ENV=development ts-node --esm webhook-server.js",
    "campaigns:test-server": "node simple-test-server.js",
    "campaigns:check-env": "node scripts/check-env.js",
    "campaigns:db-setup": "node scripts/run-migration.js ./migrations/db_setup.sql",
    "campaigns:migrate": "node scripts/run-migration.js ./migrations/campaign_activity_tracking.sql",
    "campaigns:create-tables": "node scripts/create-campaign-tables.js",
    "campaigns:test": "node scripts/test-campaign-scheduler.js",
    "campaigns:setup": "npm run campaigns:check-env && ((npm run campaigns:db-setup && npm run campaigns:migrate) || npm run campaigns:create-tables) && npm run campaigns:run",
    "campaigns:deploy": "node scripts/production-deploy.js",
    "campaigns:sql": "echo 'Please run the SQL in migrations/production_campaign_setup.sql in the Supabase dashboard'",
    "campaigns:start": "node scripts/deploy-now.js",
    "campaigns:simple": "node simple-scheduler.js",
    "campaigns:production": "node production-scheduler.js",
    "dev:campaigns": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"npm run campaigns:scheduler\"",
    "dev:stripe": "cross-env NODE_ENV=development STRIPE_TEST_MODE=true concurrently \"npm run dev\" \"npm run webhook\" \"stripe listen --forward-to http://localhost:4242/api/stripe/webhook\"",
    "dev:all": "cross-env NODE_ENV=development STRIPE_TEST_MODE=true concurrently \"npm run dev\" \"npm run webhook\" \"stripe listen --forward-to http://localhost:4242/api/stripe/webhook\""
  },
  "dependencies": {
    "@clerk/backend": "^1.24.2",
    "@clerk/clerk-react": "^5.23.0",
    "@clerk/nextjs": "^6.12.0",
    "@clerk/types": "^4.46.1",
    "@radix-ui/react-slot": "^1.1.2",
    "@sentry/node": "^7.92.0",
    "@sentry/profiling-node": "^1.2.0",
    "@stripe/stripe-js": "^5.6.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.24.1",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.0",
    "@types/react-datepicker": "^6.0.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.3",
    "chart.js": "^4.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "date-fns": "^3.3.1",
    "dompurify": "^3.2.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.1",
    "firebase": "^11.3.1",
    "framer-motion": "^12.5.0",
    "helmet": "^7.1.0",
    "lucide-react": "^0.344.0",
    "next": "^15.1.7",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^6.1.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.2",
    "recharts": "^2.15.1",
    "stripe": "^17.7.0",
    "svix": "^1.60.0",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "supabase": "^1.145.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
